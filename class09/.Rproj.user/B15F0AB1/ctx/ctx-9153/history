library(bio3d)
hiv <- read.pdb("1hsg")
hiv
# check out data
head(hiv$atom)
# run Blast on chain A sequence
chainA <- trim.pdb(hiv, chain="A")
chainA.seq <- pdbseq(chainA)
blast <- blast.pdb(chainA.seq)
blast$hit.tbl
# plot Blast results
hits <- plot(blast)
hits$pdb.id
blast <- blast.pdb(chainA.seq)
